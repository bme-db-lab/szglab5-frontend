<div class="entry-test-list">
  {{#unless showSettings}}
    {{#if model.tests}}
      {{table-component header=header body=getBody rowIndecies=rowIndecies withDelete=true withSettings=true openSettings=(action 'openSettings') delete=(action 'delete')}}
    {{else}}
      Jelenleg nincs kerdescsoport az adatbazisban. Hozzon letre egyet!
    {{/if}}
  {{/unless}}
  {{#if showSettings}}
    {{entry-test-creator entryTest=entryTest edit=true closeSettings=(action "closeSettings") model=model}}
  {{/if}}
</div>
