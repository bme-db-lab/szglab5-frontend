<div class="entry-test-list">
  {{#unless showSettings}}
    {{object-table-component header=header body=body rowIndecies=rowIndecies withDelete=true withSettings=true openSettings=(action 'openSettings')}}
  {{/unless}}
  {{#if showSettings}}
    {{entry-test-creator entryTest=entryTest edit=true closeSettings=(action "closeSettings")}}
  {{/if}}
</div>
