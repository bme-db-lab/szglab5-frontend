<div class="statistics-table">
    <table class='table table-striped table-sm table-bordered table-hover'>
        <thead class="thead-inverse">
          <th>#</th>
            {{#each header as |head|}}
                <th>{{head}}</th>
            {{/each}}
        </thead>
        <tbody>
            {{#each body as |row number|}}
                <tr>
                    <th scope='row'>{{add number}}</th>
                    {{#each row as |cell|}}
                        <td>{{cell}}</td>
                    {{/each}}
                </tr>
            {{/each}}
        </tbody>
    </table>
</div>
