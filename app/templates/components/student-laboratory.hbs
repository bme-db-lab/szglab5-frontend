<div class='student-laboratory'>
  <h4>Helyszín</h4>
  <p>{{result.location}}</p>
  <h4>Időpont</h4>
  <p>{{result.date}}</p>
  <h4>Laborvezető</h4>
  <p>{{await result.Demonstrator.displayName}} ({{await result.Demonstrator.email}})</p>
  {{#each result.Deliverables as |Deliverable number|}}
    <h3 class="text label">{{Deliverable.DeliverableTemplate.description}}</h3>
    <p><strong>Határidő:</strong> {{Deliverable.deadline}}</p>
    <p><strong>Feltöltve:</strong> {{Deliverable.submitteddate}}</p>
    <p><strong>Jegy:</strong> {{Deliverable.grade}}</p>
    <p><strong>Megjegyzés:</strong> {{Deliverable.comment}}</p>
    <p><strong>Git:</strong> {{Deliverable.url}} {{Deliverable.commit}}</p>
    <div class="tagSelector">
      {{#power-select options=result.commits selected=result.finalcommit onchange=(action "selectCommit") as |commit|}}
      {{commit}}
      {{/power-select}}
    </div>
  {{/each}}

  {{#if (eq result.statusCode 'after')}}
    <div class="text label">
      Beugró érdemjegye
    </div>
    <div class="text">
      {{result.entrytest}} ({{result.demonstrator}})
    </div>
    <div class="text label">
      Jegyzőkönyv érdemjegye
    </div>
    <div class="text" id="afterreportgrade">
      {{result.report.grade}} ({{result.report.evaluator}})
    </div>
    <div class="text label">
      Jegyzőkönyv megjegyzés
    </div>
    <div class="text long">
      {{result.report.review}}
    </div>
    <div class="text label">
      Labor érdemjegye
    </div>
    <div class="text" id="afterlaboratorygrade">
      {{result.laboratory.grade}} ({{result.demonstrator}})
    </div>
    <div class="text label">
      Labor megjegyzés
    </div>
    <div class="text long">
      {{result.laboratory.review}}
    </div>
  {{/if}}
</div>
